{
    "name": "finet-sid/xsd2php",
    "description": "Fork of goetas-webservices/xsd2php",
    "type": "library",
    "authors": [
        {
            "name": "Simon Demele",
            "email": "simon.demele@finet.de"
        }
    ],
    "keywords": [
        "converter",
        "xml",
        "xsd",
        "php",
        "jms",
        "serializer"
    ],
    "license": "MIT",
    "require": {
        "php": "^5.5|^7.0",
        "symfony/console": "^2.7|^3.0|^4.0",
        "symfony/dependency-injection": "^2.2|^3.0|^4.0",
        "symfony/yaml": "^2.1|^3.0|^4.0",
        "symfony/config": "^2.2|^3.0|^4.0",
        "goetas-webservices/xsd-reader": "^0.2|^0.3.1",
        "doctrine/inflector": "^1.0",
        "zendframework/zend-code": "^3.0.3",

        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0",
        "jms/serializer": "^1.9",
        "goetas-webservices/xsd2php-runtime": "^0.2.7",
        "ext-xmldiff": "*"
    },
    "autoload": {
        "psr-4": {
            "GoetasWebservices\\Xsd\\XsdToPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "bin": [
        "bin/xsd2php"
    ]
}
